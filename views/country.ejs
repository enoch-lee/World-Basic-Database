<% include partials/header %>

<div class="container">
	<table id = 'data_table' class="table table-sm table-hover table-striped" style="width: 100%; margin: auto">
		<thead>
			<tr>
				<th>Name</th>
				<th>Continent</th>
				<th>Region</th>
        <th>SurfaceArea</th>
        <th>IndepYear</th>
        <th>Population</th>
        <th>LifeExpectancy</th>
        <th>GNP</th>
        <th>GovernmentForm</th>
			</tr>
		</thead>
		<tbody>
			<% results.forEach(function(result){ %>
			<tr>
				<td><a class ="country_name" href = "" ><%= result.Name %></a></td>
				<td><%= result.Continent %></td>
				<td><%= result.Region %></td>
        <td><%= result.SurfaceArea %></td>
        <td><%= result.IndepYear %></td>
        <td><%= result.Population %></td>
        <td><%= result.LifeExpectancy %></td>
        <td><%= result.GNP %></td>
        <td><%= result.GovernmentForm %></td>
			</tr>
			<% }) %>
		</tbody>
	</table>
	<p></p>
	<p><a id = 'edit' href=" ">Edit</a></p> 	<a href='/'>Back</a>
</div>

<script type="text/javascript" charset="utf8" src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.8.2.min.js"></script>
<script>
	let route = '/edit/' + <%- JSON.stringify(results[0].Name) %>
	$('#edit').attr('href', route);
</script>

<% include partials/footer %>
